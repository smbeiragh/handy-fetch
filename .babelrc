{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "8",
              "browsers": [
                "last 30 versions",
                "chrome >= 30",
                "firefox >= 25",
                "safari >= 7",
                "ie >= 10",
                "edge >= 12"
              ]
            }
          }
        ]
      ],
      "plugins": [
        "babel-plugin-istanbul"
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "8",
              "browsers": [
                "last 30 versions",
                "chrome >= 30",
                "firefox >= 25",
                "safari >= 7",
                "ie >= 10",
                "edge >= 12"
              ]
            },
            "modules": false
          }
        ]
      ],
      "plugins": [
      ]
    }
  }
}
